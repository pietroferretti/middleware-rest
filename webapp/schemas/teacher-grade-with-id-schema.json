{
    "url_template": "/teacher/{teacher_id}/class/{class_id}/subject/{subject_id}/grade/{grade_id}/",
    "template_parameters": [
        {
            "class_id": {
                "type": "integer",
                "entity": "http://entities.backtoschool.io/classid"
            }
        },
        {
            "teacher_id": {
                "type": "integer",
                "entity": "http://entities.backtoschool.io/teacherid"
            }
        },
        {
            "subject_id": {
                "type": "integer",
                "entity": "http://entities.backtoschool.io/subjectid"
            }
        },
        {
            "grade_id": {
                "type": "integer",
                "entity": "http://entities.backtoschool.io/gradeid"
            }
        }
    ],
    "actions": [
        {
            "method": "GET",
            "outputschema": {
                "$schema": "http://json-schema.org/draft-06/schema#",
                "type": "object",
                "properties": {
                    "grade": {
                        "type": "object",
                        "properties": {
                            "id": {
                                "type": "integer",
                                "entity": "http://entities.backtoschool.io/gradeid"
                            },
                            "date": {
                                "type": "string",
                                "entity": "http://entities.backtoschool.io/date"
                            },
                            "subject_id": {
                                "type": "integer",
                                "entity": "http://entities.backtoschool.io/subjectid"
                            },
                            "student_id": {
                                "type": "string",
                                "entity": "http://entities.backtoschool.io/studentid"
                            },
                            "value": {
                                "type": "integer",
                                "entity": "http://entities.backtoschool.io/gradevalue"
                            }
                        }
                    }
                }
            }
        },
        {
            "method": "PUT",
            "inputschema": {
                "$schema": "http://json-schema.org/draft-06/schema#",
                "type": "object",
                "properties": {
                    "date": {
                        "type": "string",
                        "entity": "http://entities.backtoschool.io/date"
                    },
                    "value": {
                        "type": "integer",
                        "entity": "http://entities.backtoschool.io/gradevalue"
                    }
                },
                "required": []
            },
            "outputschema": {
                "$schema": "http://json-schema.org/draft-06/schema#",
                "type": "object",
                "properties": {
                    "grade": {
                        "type": "object",
                        "properties": {
                            "id": {
                                "type": "integer",
                                "entity": "http://entities.backtoschool.io/gradeid"
                            },
                            "date": {
                                "type": "string",
                                "entity": "http://entities.backtoschool.io/date"
                            },
                            "subject_id": {
                                "type": "integer",
                                "entity": "http://entities.backtoschool.io/subjectid"
                            },
                            "student_id": {
                                "type": "string",
                                "entity": "http://entities.backtoschool.io/studentid"
                            },
                            "value": {
                                "type": "integer",
                                "entity": "http://entities.backtoschool.io/gradevalue"
                            }
                        }
                    }
                }
            }
        },
        {
            "method": "DELETE",
            "outputschema": {
                "$schema": "http://json-schema.org/draft-06/schema#",
                "type": "object",
                "properties": {
                    "message": {
                        "constant": "Grade deleted successfully."
                    }
                }
            }
        }
    ]
}