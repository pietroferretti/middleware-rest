    {
      "parent_id": {
        "type": "integer",
        "entity": "http://entities.backtoschool.io/parentid"
      }
    }
  ],
  "actions": [
    {
      "method": "GET",
      "outputschema": {
        "$schema": "http://json-schema.org/draft-06/schema#",
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "entity": "http://entities.backtoschool.io/parentname"
          },
          "lastname": {
            "type": "string",
            "entity": "http://entities.backtoschool.io/parentlastname"
          }
        }
      }
    }
  ]
}