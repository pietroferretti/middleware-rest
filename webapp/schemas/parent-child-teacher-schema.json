{
  "url_template": "/parent/{parent_id}/child/{student_id}/teachers/",
  "template_parameters": [
    {
      "student_id": {
        "type": "integer",
        "entity": "http://entities.highschool.com/studentid"
      }
    },
    {
      "parent_id": {
        "type": "integer",
        "entity": "http://entities.highschool.com/parentid"
      }
    }
  ],
  "actions": [
    {
      "method": "GET",
      "outputschema": {
        "$schema": "http://json-schema.org/draft-06/schema#",
        "type": "object",
        "properties": {
          "teachers": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "id": {
                  "type": "integer",
                  "entity": "http://entities.highschool.com/teacherid"
                },
                "lastname": {
                  "type": "string",
                  "entity": "http://entities.highschool.com/teacherlastname"
                },
                "name": {
                  "type": "string",
                  "entity": "http://entities.highschool.com/teachername"
                },
                "subjects": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "id": {
                        "type": "integer",
                        "entity": "http://entities.highschool.com/subjectid"
                      },
                      "name": {
                        "type": "string",
                        "entity": "http://entities.highschool.com/subjectname"
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  ]
}