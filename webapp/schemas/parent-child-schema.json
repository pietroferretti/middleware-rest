{
  "url_template": "/parent/{parent_id}/child/",
  "template_parameters": [
    {
      "parent_id": {
        "type": "integer",
        "entity": "http://entities.backtoschool.io/parentid"
      }
    }
  ],
  "actions": [
    {
      "method": "GET",
      "outputschema": {
        "$schema": "http://json-schema.org/draft-06/schema#",
        "type": "object",
        "properties": {
          "students": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "lastname": {
                  "type": "string",
                  "entity": "http://entities.backtoschool.io/studentlastname"
                },
                "name": {
                  "type": "string",
                  "entity": "http://entities.backtoschool.io/studentname"
                },
                "student_id": {
                  "type": "integer",
                  "entity": "http://entities.backtoschool.io/studentid"
                }
              }
            }
          }
        }
      }
    }
  ]
}