{
    "url_template": "/teacher/{teacher_id}/appointment/",
    "template_parameters": [
        {
            "teacher_id": {
                "type": "integer",
              "entity": "http://entities.highschool.com/teacherid"
            }
        }
    ],
    "actions": [
        {
            "method": "GET",
            "outputschema": {
                "$schema": "http://json-schema.org/draft-06/schema#",
              "type": "object"
            }
        },
        {
            "method": "POST",
            "inputschema": {
                "$schema": "http://json-schema.org/draft-06/schema#",
                "type": "object",
                "properties": {
                  "date": {
                        "type": "string",
                    "entity": "http://entities.highschool.com/date"
                    },
                  "id": {
                    "type": "integer",
                    "entity": "http://entities.highschool.com/appointmentid"
                  }
                  "parent": {
                        
                    }
                },
                "required": ["username", "password"]
            },
            "outputschema": {
                "$schema": "http://json-schema.org/draft-06/schema#",
                "type": "null"
            }
        }
    ]
}
